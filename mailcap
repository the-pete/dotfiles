text/html;	firefox; nameplate=%s.html
text/html;	links -dump %s; nametemplate=%s.html; copiousoutput
application/ms-tnef; tnef -w %s
application/pdf; epdfview %s
application/pdf; pdftotext -nopgbrk %s - ; copiousoutput
application/msword; catdoc %s; copiousoutput
application/msword; libreoffice %s
text/calendar; mutt-ical.py -i -e "pbaldrid@amazon.com" %s
text/calendar; dump-ical.py %s ; copiousoutput
image/*; feh -x %s ; edit=gimp %s
image/gif; mplayer -loop 0 %s
image/jpeg; image2ascii %s ; copiousoutput
video/* ; mplayer %s
